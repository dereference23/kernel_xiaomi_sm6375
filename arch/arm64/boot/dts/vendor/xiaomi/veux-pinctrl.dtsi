&tlmm {
	nfc {
		nfc_clk_req_active: nfc_clk_req_active {
			mux {
				pins = "gpio7";
				function = "gpio";
			};

			config {
				pins = "gpio7";
				drive-strength = <2>;
				bias-pull-up;
			};
		};

		nfc_clk_req_suspend: nfc_clk_req_suspend {
			mux {
				pins = "gpio7";
				function = "gpio";
			};

			config {
				pins = "gpio7";
				drive-strength = <2>;
				bias-disable;
			};
		};
	};

	qupv3_se7_spi_pins: qupv3_se7_spi_pins {
		qupv3_se7_spi_active: qupv3_se7_spi_active {
			mux {
				pins = "gpio27", "gpio28";
				function = "qup11_f1";
			};

			config {
				pins = "gpio27", "gpio28";
				drive-strength = <6>;
				bias-disable;
			};
		};

		qupv3_se7_spi_sleep: qupv3_se7_spi_sleep {
			mux {
				pins = "gpio27", "gpio28";
				function = "gpio";
			};

			config {
				pins = "gpio27", "gpio28";
				drive-strength = <6>;
				bias-disable;
			};
		};
	};
};

&nfc_int_active {
	config {
		/delete-property/ bias-pull-down;
		bias-pull-up;
	};
};

&nfc_int_suspend {
	config {
		/delete-property/ bias-pull-down;
		bias-pull-up;
	};
};

&nfc_enable_active {
	config {
		/delete-property/ bias-disable;
		bias-pull-up;
	};
};

&qupv3_se0_i2c_active {
	config {
		input-enable;
	};
};

&qupv3_se0_i2c_sleep {
	config {
		input-enable;
	};
};

&qupv3_se8_i2c_active {
	config {
		drive-strength = <8>;
	};
};
